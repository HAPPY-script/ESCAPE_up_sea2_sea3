
local placeId = game.PlaceId


if placeId == 4442272183 then
    
    loadstring(game:HttpGet("https://raw.githubusercontent.com/HAPPY-script/ESCAPE_up_sea2/refs/heads/main/ESCAPE_up_sea2"))()
elseif placeId == 7449423635 then
    
    loadstring(game:HttpGet("https://raw.githubusercontent.com/HAPPY-script/ESCAPE_up_sea3/refs/heads/main/ESCAPE_up_sea3"))()
else
    warn("Không phải ở Sea 2 hoặc Sea 3. Script không chạy.")
end
